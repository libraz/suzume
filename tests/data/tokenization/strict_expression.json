{
  "version": "1.0",
  "cases": [
    {
      "id": "mixed_english_api",
      "description": "API with を particle",
      "input": "APIを呼ぶ",
      "expected": [
        {"surface": "api", "pos": "Noun"},
        {"surface": "を", "pos": "Particle"},
        {"surface": "呼ぶ", "pos": "Verb"}
      ],
      "tags": ["strict", "mixed", "english"]
    },
    {
      "id": "mixed_camelcase",
      "description": "CamelCase function name preserved with を particle",
      "input": "getUserDataを呼ぶ",
      "expected": [
        {"surface": "getuserdata", "pos": "Noun"},
        {"surface": "を", "pos": "Particle"},
        {"surface": "呼ぶ", "pos": "Verb"}
      ],
      "tags": ["strict", "mixed", "english"]
    },
    {
      "id": "mixed_digit_unit",
      "description": "3人 with particle で",
      "input": "3人で行く",
      "expected": [
        {"surface": "3人", "pos": "Noun"},
        {"surface": "で", "pos": "Particle"},
        {"surface": "行く", "pos": "Verb"}
      ],
      "tags": ["strict", "mixed", "number"]
    },
    {
      "id": "contraction_shiteru",
      "description": "してる should contain verb component",
      "input": "してる",
      "expected": [
        {"surface": "してる", "pos": "Verb"}
      ],
      "tags": ["strict", "contraction"]
    },
    {
      "id": "contraction_miteta",
      "description": "見てた - unified contracted form of 見ていた",
      "input": "見てた",
      "expected": [
        {"surface": "見てた", "pos": "Verb", "lemma": "見る"}
      ],
      "tags": ["strict", "contraction"]
    },
    {
      "id": "symbol_brackets",
      "description": "AI（人工知能）- should recognize AI and 人工知能",
      "input": "AI（人工知能）",
      "expected": [
        {"surface": "ai", "pos": "Noun"},
        {"surface": "人工知能", "pos": "Noun"}
      ],
      "tags": ["strict", "symbol"]
    },
    {
      "id": "symbol_quotation",
      "description": "「こんにちは」- should recognize こんにちは",
      "input": "「こんにちは」",
      "expected": [
        {"surface": "こんにちは", "pos": "Other"}
      ],
      "tags": ["strict", "symbol"]
    },
    {
      "id": "complex_technical",
      "description": "Python technical sentence with particles",
      "input": "Pythonで機械学習を実装する",
      "expected": [
        {"surface": "python", "pos": "Noun"},
        {"surface": "で", "pos": "Particle"},
        {"surface": "機械学習", "pos": "Noun"},
        {"surface": "を", "pos": "Particle"},
        {"surface": "実装する", "pos": "Verb"}
      ],
      "tags": ["strict", "complex", "technical"]
    },
    {
      "id": "complex_business",
      "description": "Business request - prefix splits from nouns",
      "input": "ご確認をお願いいたします",
      "expected": [
        {"surface": "ご", "pos": "Prefix"},
        {"surface": "確認", "pos": "Noun"},
        {"surface": "を", "pos": "Particle"},
        {"surface": "お", "pos": "Prefix"},
        {"surface": "願い", "pos": "Noun"},
        {"surface": "いたします", "pos": "Verb", "lemma": "いたす"}
      ],
      "tags": ["strict", "complex", "business"]
    },
    {
      "id": "realworld_business_email",
      "description": "お世話になっております - humble progressive form",
      "input": "お世話になっております",
      "expected": [
        {"surface": "お", "pos": "Prefix"},
        {"surface": "世話", "pos": "Noun"},
        {"surface": "に", "pos": "Particle"},
        {"surface": "なって", "pos": "Verb", "lemma": "なる"},
        {"surface": "おります", "pos": "Verb", "lemma": "おる"}
      ],
      "tags": ["strict", "realworld", "business"]
    },
    {
      "id": "realworld_shopping",
      "description": "これはいくらですか - should have particles",
      "input": "これはいくらですか",
      "expected": [
        {"surface": "これ", "pos": "Pronoun"},
        {"surface": "は", "pos": "Particle"},
        {"surface": "いくら", "pos": "Pronoun"},
        {"surface": "です", "pos": "Auxiliary"},
        {"surface": "か", "pos": "Particle"}
      ],
      "tags": ["strict", "realworld", "conversation"]
    },
    {
      "id": "realworld_weather",
      "description": "今日は暑いですね - should recognize 今日",
      "input": "今日は暑いですね",
      "expected": [
        {"surface": "今日", "pos": "Noun"},
        {"surface": "は", "pos": "Particle"},
        {"surface": "暑い", "pos": "Adjective"},
        {"surface": "です", "pos": "Auxiliary"},
        {"surface": "ね", "pos": "Particle"}
      ],
      "tags": ["strict", "realworld", "conversation"]
    },
    {
      "id": "realworld_technical",
      "description": "ファイルが見つかりませんでした - should recognize が particle",
      "input": "ファイルが見つかりませんでした",
      "expected": [
        {"surface": "ファイル", "pos": "Noun"},
        {"surface": "が", "pos": "Particle"},
        {"surface": "見つかり", "pos": "Verb", "lemma": "見つかる"},
        {"surface": "ません", "pos": "Auxiliary"},
        {"surface": "でした", "pos": "Auxiliary", "lemma": "でした"}
      ],
      "tags": ["strict", "realworld", "technical"]
    }
  ]
}
